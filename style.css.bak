
body{
	display: fixed;
	align-items: center;
	justify-content: center;
}

.parent{
	min-height: 100vh;
	z-index: -1;
}
.main{
	width: 360px;
	height: 640px;
	background: linear-gradient( 
		to bottom,
		#ef720b 0%,
		#ef720b 40%,
		white 40%,
		white 100%
	);
	border: 1px solid #000;
	border-radius: 3%;
}
img{
	width: 50px;
}

.faq-section{
	margin: 2%;
	background-color: #fff;
	z-index: -1;
	height: 400px;
	border: 1px solid #000;
	color: #000;
	border-radius: 3%;
}

.icon-robot{
	transform: translate(50%,-50%);
	width: 70px;
	height: 70px;
	font-size: 35px;
	color: #fff;
	display: inline-flex;
	align-items: center;
	justify-content: center;
	background: #f87217;
	border-radius: 50%;
	box-shadow: 0px -8px 20px rgba(0,0,0,0.3);
	z-index: -1;
}

.faq{
	margin-top: -5%;
	margin-left: 5%;
}

.question-icon{
	width: 20px;
	height: 20px;
	font-size: 20px;
	font-weight: bold;
	color: #f87217;
	align-items: center;
	justify-content: center;
	background: #;
	border-radius: 50%;
}

.faq-box{
	border: 1px solid #000;
	margin-top: 15px;
	border-radius: 6px;
	margin-right: 5%;
	height: 40px;
}

.dot {
	margin-left: 5%;
	margin-right: 5%;
	margin-top: 5%;
	height: 10px;
	float: left;
	width: 10px;
	background-color: #f87217;
	border-radius: 50%;
	display: inline-block;
	display: relative;
}

.faq-box p{
	margin-top: 3.5%;
	padding-left: 10px;
	font-size: 0.8em;
	float: center;
}

.button{
	padding: 8px 10px;
	border-radius: 27px;
	background-color: white;
	border: 1px solid #000;
	text-align: center;
	margin-top: 10%;
	margin-right: 15%;
	margin-left: 10%;
	box-shadow: 0px 7px 7px rgba(0,0,0,0.2);
	
}

.button a{
	font-weight: bold;
	font-size: 0.9em;
}

.button img {
	height: 20px;
	width: 20px;
}

.faq{
	animation: slide-up 3.5s;
}

.container{
	animation: slide-up 3s;
}
@keyframes slide-up {
		from{
			opacity: 0.2;
			line-height: 200px;
		}	
		to{
			opacity: 1;
			line-height: 25px;
		}
}